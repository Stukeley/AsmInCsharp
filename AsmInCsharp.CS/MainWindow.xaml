<Window x:Class="AsmInCsharp.CS.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AsmInCsharp.CS"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
	<Grid>
		<StackPanel VerticalAlignment="Center" HorizontalAlignment="Center" Width="500">
			<TextBlock x:Name="ExpectedValueBlock" Text="Expected value is:" FontSize="16" Margin="10" HorizontalAlignment="Center" TextAlignment="Center"/>
			
			<StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
				<Button x:Name="RunAsmButton" Content="ASM" FontSize="16" Margin="5" Click="RunAsmButton_Click"/> 
			</StackPanel>

			<TextBlock x:Name="ResultBlock" Margin="10" FontSize="16" HorizontalAlignment="Center"/>
		</StackPanel>
	</Grid>
</Window>
